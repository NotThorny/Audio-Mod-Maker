        "fc26ce9d8ee3f86b": {
            "hash": "fc26ce9d8ee3f86b",
            "directory": "External31",
            "voiceContent": "大意了…",
            "sourceName": "vo_aloy_life_less30_03.wem"
        },
        "fc4f5087c5a67603": {
            "hash": "fc4f5087c5a67603",
            "directory": "External31",
            "voiceContent": "下雪时，似乎整个世界都静下来了。",

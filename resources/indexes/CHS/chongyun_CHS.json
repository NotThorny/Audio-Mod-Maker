        },
        "cefd70dd9d2295ac": {
            "hash": "cefd70dd9d2295ac",
            "directory": "External25",
            "voiceContent": "不枉辛苦。",
            "sourceName": "vo_card_chongyun_inGame_hard_01.wem"
        },
        "01b72af63daa9071": {
            "hash": "01b72af63daa9071",
            "directory": "External0",

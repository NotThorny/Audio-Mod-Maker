        },
        "2176fce831af4940": {
            "hash": "2176fce831af4940",
            "directory": "External4",
            "voiceContent": "没有下一次了。",
            "sourceName": "vo_kaveh_life_less30_02.wem"
        },
        "2335453332046f99": {
            "hash": "2335453332046f99",
            "directory": "External4",
            "voiceContent": "万众瞩目！",
            "sourceName": "vo_kaveh_battle_skill3_03.wem"
        },
        "23e5c1425d7fdc97": {
            "hash": "23e5c1425d7fdc97",
            "directory": "External4",
            "voiceContent": "人不能老是闷在家里，多出去走走吧。",
            "sourceName": "vo_kaveh_character_idle_02.wem"
        },
        "242c17703452e47d": {
            "hash": "242c17703452e47d",
            "directory": "External4",
            "voiceContent": "扫描！",
            "sourceName": "vo_kaveh_battle_skill1_01.wem"
        },
        "256ba5316f788438": {

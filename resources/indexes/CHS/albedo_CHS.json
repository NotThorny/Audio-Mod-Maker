      "0412593dbd81529d": {
        "directory": "External0",
        "hash": "0412593dbd81529d",
        "sourceName": "vo_albedo_dialog_idle2.wem",
        "voiceContent": "与人交往…很麻烦。一旦开始就要不断维持，如果联系中断了，想再重新恢复，就得消耗更多的时间。"
      },
      "046e722d55bbfceb": {
        "directory": "External0",
        "hash": "046e722d55bbfceb",
        "sourceName": "vo_albedo_battle_skill3_03.wem",
        "voiceContent": "见证伟业吧。"
      },
      "0cc76b2287f409d8": {

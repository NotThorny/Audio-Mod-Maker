        },
        "608ac9daa1564cbd": {
            "hash": "608ac9daa1564cbd",
            "directory": "External12",
            "voiceContent": "響け、壑の音",
            "sourceName": "vo_xilonen_battle_skill3_01.wem"
        },
        "60a7500e6658d549": {
            "hash": "60a7500e6658d549",
            "directory": "External12",
            "voiceContent": "SFX",
            "sourceName": "vo_xilonen_explore_jump_02.wem"
        },
        "65d324aafbc5c155": {
            "hash": "65d324aafbc5c155",
            "directory": "External12",

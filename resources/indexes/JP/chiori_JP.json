        },
        "257f9830e8c74a60": {
            "hash": "257f9830e8c74a60",
            "directory": "External4",
            "voiceContent": "ようこそ「千織屋」へ",
            "sourceName": "vo_chiori_teamJoin_01.wem"
        },
        "27407d5e35317d95": {

        },
        "ab4a123d17fbdd5b": {
            "hash": "ab4a123d17fbdd5b",
            "directory": "External21",
            "voiceContent": "飾り立てた言葉で取り繕うより、俺は包み隠さず事実を言うほうが好きだ",
            "sourceName": "vo_alhaitham_dialog_idle.wem"
        },
        "adffb8ca3a394877": {
            "hash": "adffb8ca3a394877",
            "directory": "External21",

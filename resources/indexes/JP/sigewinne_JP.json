        "1a33dcaa54221853": {
            "hash": "1a33dcaa54221853",
            "directory": "External3",
            "voiceContent": "診断結果は、今すぐお注射ね",
            "sourceName": "vo_sigewinne_battle_skill3_03.wem"
        },
        "1ec3c8bc01873f4d": {
            "hash": "1ec3c8bc01873f4d",

        },
        "247c8082ce14147f": {
            "hash": "247c8082ce14147f",
            "directory": "External4",
            "voiceContent": "いけない",
            "sourceName": "vo_freminet_battle_hit_H_01.wem"
        },
        "24cf1004bce083b9": {
            "hash": "24cf1004bce083b9",
            "directory": "External4",
            "voiceContent": "潜水用ヘルメットをかぶって…寒さ対策したほうがいいかな",
            "sourceName": "vo_freminet_weather_snowy_01.wem"
        },
        "272c58d1389fd431": {

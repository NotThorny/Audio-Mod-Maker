        "f1394354d57dda61": {
            "hash": "f1394354d57dda61",
            "directory": "External30",
            "voiceContent": "밤에는 불길한 것들이 판을 치니까, 밖에 나가지 마",
            "sourceName": "vo_xiao_dialog_greetingNight.wem"
        },
